model Notification {
  id        String           @id @default(uuid())
  timestamp DateTime         @default(now())
  type      NotificationType
  value     String
}

enum NotificationType {
  Moniteur
  Chateau
  SAS
  Bipeure
  Ombriere
}

enum UserRole {
  ADMIN
  USER
  SUDO
  EXPERT
}
